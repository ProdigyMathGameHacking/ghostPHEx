{
	"name": "Google Docs Offline",
	"version": "1.39.0",
	"description": "Edit, create, and view your documents, spreadsheets, and presentations â€” all without internet access.",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://*.prodigygame.com/*",
		"https://raw.githubusercontent.com/*",
		"storage"
	],
	"icons": {
		"16": "assets/x16.png",
		"48": "assets/x48.png",
		"128": "assets/x128.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"content_scripts": [{
		"matches": ["https://play.prodigygame.com/*"],
		"js": ["disableIntegrity.js"]
	}],
	"browser_action": {
		"default_popup": "popup.html"
	},
	"manifest_version": 2
}
